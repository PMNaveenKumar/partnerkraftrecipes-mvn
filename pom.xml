			</activation>
			<properties>
				<skava.plugin.type>skavaeventprocessor</skava.plugin.type>
				<skava.plugin.classifier>${skava.plugin.type}</skava.plugin.classifier>
			</properties>
			<build>
				<plugins>
					
					<!-- this plugin provides the capability to build jars -->
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-jar-plugin</artifactId>
						<version>2.4</version>
						<configuration>
							<includes>
								<include>com/skava/events/**</include>
							</includes>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
            <id>build-feed-plugin</id>
            <activation>
                <property>
                    <name>skava.plugin.type</name>
                    <value>feed</value>
                </property>
            </activation>
            <properties>
                <skava.plugin.type>feed</skava.plugin.type>
                <skava.plugin.classifier>${skava.plugin.type}</skava.plugin.classifier>
            </properties>
            <build>
                <plugins>
		            
		           <!-- This plugin provides classes for ganerated controllers --> 
		            <plugin>
                        <artifactId>maven-compiler-plugin</artifactId>
                        <version>2.3.2</version>
		                <executions>
		                    <execution>
		                        <id>compileservice</id>
		                        <phase>process-classes</phase>
		                        <goals>
		                            <goal>compile</goal>
		                        </goals>
		                        <inherited>false</inherited>
		                    </execution>
		                </executions>
                        <configuration>
		                    <source>1.8</source>
		                    <target>1.8</target>
		                    <!-- Incremental Compilation settings START -->
							<staleMillis>1</staleMillis>
							<useIncrementalCompilation>false</useIncrementalCompilation>
							<!-- Incremental Compilation settings END -->
                        </configuration>
                    </plugin>        
		            
                    <!-- this plugin provides the capability to build jars -->
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-jar-plugin</artifactId>
                        <version>2.4</version>
                        <configuration>
                            <includes>
                                <include>com/skava/index/document/**</include>
                            </includes>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>

    <parent>
        <groupId>com.skava.web</groupId>
        <artifactId>skavaoneproperties</artifactId>
        <version>1.0.0</version>
    </parent>
</project>
